    this.setEnable = function(isEnable,sFieldId)
    {
        var timeTxtObj = document.getElementById(sFieldId);
        //alert(isEnable)
        if(isEnable)
        {
            if(timeTxtObj.disabled || timeTxtObj.getAttribute("disabled"))
