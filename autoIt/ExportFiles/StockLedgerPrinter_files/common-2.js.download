        else if(cross.getKeyCode(evnt)== 38) //up arrow
            me.onChangeAMPM(sTimePickerId,true);
        else if(cross.getKeyCode(evnt)==9)
            {
                me.onChangeKeyAMPM(sTimePickerId+"ampm",sTimePickerId,evnt);
                me.onChangeKeyAMPM(sTimePickerId+"ampm",sTimePickerId,evnt);
