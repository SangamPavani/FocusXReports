                        || iExtraIdStatus==byCheckStatus3 || iExtraIdStatus==byCheckStatus4 || iExtraIdStatus==byCheckStatus5
                        || iExtraIdStatus==byCheckStatus6 || (byActionId==Constants.ASSIGN && iExtraIdStatus==byCheckStatus7))
