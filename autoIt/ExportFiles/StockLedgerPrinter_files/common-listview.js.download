        /*if(sValue.indexOf("$")!=-1)
            sParam= "&globalSearch=" + sValue.substring(sValue.indexOf("$")+1);
        else if(sValue.indexOf("#")!=-1)
            sParam= "&tagSearch=" + sValue.substring(sValue.indexOf("#")+1);
