var MsgHandler = new function()
{

    this.getMsg=function(iMsgId,iActionId,iModuleId)
    {
        var args=new Array();

        //var lang=clAppBuffer.getLanguage();
        args[0]=Focus.crm.Messages.get(iMsgId,iActionId,iModuleId);
        for(var i=3; i < arguments.length;i++)
            args[i-2]=arguments[i];

//        alert("getting msg  "+args[0])
        return replaceMsgVariables(args);
    };

    this.getModuleActionLabel=function(iModuleId,iActionId,isGetSingularName)
    {
//        alert("iModuleId==="+iModuleId);
//        alert(clAppBuffer.getModuleCaption(iModuleId,isGetSingularName));
        if(iActionId>0)
            return Focus.crm.Messages.getActionLabel(iActionId)+" "+clAppBuffer.getModuleCaption(iModuleId,isGetSingularName);
        else
            return clAppBuffer.getModuleCaption(iModuleId,isGetSingularName);

    };

    this.getStatusMsg=function(iAppActionId)
    {
        return Focus.crm.Messages.getStatusMsg(iAppActionId);
    };


    function replaceMsgVariables(args)
    {
        var sMsg=args[0];

        if(args.length>1)
        {
            for(var iIndex=1; iIndex < args.length; iIndex++)
            {
                while(sMsg.indexOf("#"+(iIndex-1)+"#")!=-1)
                    sMsg=sMsg.replace("#"+(iIndex-1)+"#",args[iIndex]);
            }
        }

        if(sMsg.indexOf("#")!=-1)
            sMsg=sMsg.replace(/(#[0-9]+#)/g, "");
        return sMsg;
    };

};




